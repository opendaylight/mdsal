module recursive-uses-augment {

    namespace "yang:test:uses-augment-recursive";
    prefix ruses;

    revision 2017-05-19;

    grouping A {
        container A1 {
            leaf leaf-A1 {
                type string;
            }
        }
    }

    grouping B {
        container B1 {
            leaf leaf-B1 {
                type string;
            }
        }
    }

    grouping C {
        container C1 {
            leaf leaf-C1 {
                type string;
            }
        }
