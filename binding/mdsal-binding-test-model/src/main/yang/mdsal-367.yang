module mdsal-367.yang {
    namespace "mdsal-367";
    prefix "mdsal367";

    identity someid;

    container foo {
        leaf bit-leaf {
            type bits {
                bit foo;
            }
        }
        leaf decimal-leaf {
            type decimal64 {
                fraction-digits 5;
            }
        }
        leaf identityref-leaf {
            type identityref {
                base someid;
            }
        }
        leaf string-leaf {
            type string {
                length 5;
            }
        }
        leaf union-leaf {
            type enumeration {
                enum foo;
            }
        }
    }

    container bar {
